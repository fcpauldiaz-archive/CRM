{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Reportes de detalle</h1>
    <h2>Cliente Membresia</h2>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Cantidad</th>
                <th>Total</th>
                <th>Fecha</th>
                <th>Producto</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in venta_producto %}
            <tr>
                <td>{{ entity.0 }}</td>
                <td>{{ entity.1 }}</td>
                <td>{{ entity.2 }}</td>
                <td>{{ entity.3 }}</td>
                <td>{{ entity.4 }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    

    {% endblock %}
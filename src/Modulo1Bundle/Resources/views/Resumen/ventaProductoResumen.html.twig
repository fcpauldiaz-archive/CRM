{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Reportes de detalle</h1>

    {{form(form)}}
    <h2>Cliente Membresia</h2>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Cantidad</th>
                <th>Total</th>
                <th>Fecha</th>
                <th>Producto</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in venta_producto %}
            <tr>
                <td>{{ entity['id'] }}</td>
                <td>{{ entity['cantidad'] }}</td>
                <td>{{ entity['total'] }}</td>
                <td>{{ entity['fecha'] }}</td>
                <td>{{ entity['producto'] }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    

    {% endblock %}
{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Reportes de detalle</h1>

    {{ form(form)}}


    <h2>Cliente Membresia</h2>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Apellidos</th>
                <th>DPI</th>
                <th>Tipo Mempbresia</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in cliente_membresia %}
            <tr>
                <td>{{ entity['id'] }}</td>
                <td>{{ entity['nombres'] }}</td>
                <td>{{ entity['apellidos'] }}</td>
                <td>{{ entity['dpi'] }}</td>
                <td>{{ entity['tipo_membresia'] }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    

    {% endblock %}